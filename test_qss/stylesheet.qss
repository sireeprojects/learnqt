QPushButton {
    background-color: rgb(70,70,70);
    color: white;
    margin: 0px;
    border-radius: 5px;
    }

QPushButton:pressed {
    background-color: rgb(70,70,70);
    color: white;
    margin: 2px;
    border: 2px solid blue;
    }

QPushButton:hover {
    background-color: rgb(150,150,150);
    color: white;
    border: 2px solid red;
    }
